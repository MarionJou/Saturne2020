    <class>org.centrale.pgrou.items.Quiz</class>
    <class>org.centrale.pgrou.items.Test</class>
    <class>org.centrale.pgrou.items.Personne</class>
    <class>org.centrale.pgrou.items.Contenuquiz</class>
    <class>org.centrale.pgrou.items.Notation</class>
